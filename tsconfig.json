{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": "./src",
    "paths": {
      "@app/*": ["app/*"],
      "@components/*": ["app/components/*"],
      "@layouts/*": ["app/layouts/*"],
      "@models/*": ["app/models/*"],
      "@pages/*": ["app/pages/*"],
      "@services/*": ["app/services/*"],
      "@sharedModules/*": ["app/shared-modules/*"],
      "@store/*": ["app/store/*"],
      "@utils/*": ["app/utils/*"],
      "@facades/*": ["app/facades/*"],
      "@interfaces/*": ["app/interfaces/*"],
      "@assets/*": ["assets/*"]
    },
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
